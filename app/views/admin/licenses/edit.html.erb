<%= form_for @license, url: admin_license_path(@license), method: :put do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :capacity %>
  <%= f.text_field :capacity %>
  <%= f.submit %>
<% end %>

<hr>
<h4>Installed on:</h4>
<ul>
  <% @license.machines.each do |machine| %>
  <%= link_to machine.barcode, machine_path(machine) %>
  <% end %>
</ul>